{
    "version": "0.2.0",
    "configurations": [
      {
        "name": "Start lesson3",
        "type": "node",
        "request": "launch",
        "cwd": "${workspaceFolder}",
        "runtimeExecutable": "npx",
        "windows": {
          "runtimeExecutable": "npx.cmd"
        },
        "runtimeArgs": [
          "fiori",
          "run"
        ],
        "args": [
          "--open",
          "test/flpSandbox.html#lesson3-display"
        ],
        "console": "internalConsole",
        "internalConsoleOptions": "openOnSessionStart",
        "outputCapture": "std",
        "env": {
          "DEBUG": "--inspect",
          "FIORI_TOOLS_URL_PARAMS": "sap-ui-xx-viewCache=false",
          "run.config": "{\"handlerId\":\"fiori_tools\",\"runnableId\":\"/home/user/projects/lesson3\"}"
        }
      },
      {
        "name": "Start lesson3 Local",
        "type": "node",
        "request": "launch",
        "cwd": "${workspaceFolder}",
        "runtimeExecutable": "npx",
        "windows": {
          "runtimeExecutable": "npx.cmd"
        },
        "runtimeArgs": [
          "fiori",
          "run"
        ],
        "args": [
          "--config",
          "./ui5-local.yaml",
          "--open",
          "test/flpSandbox.html#lesson3-display"
        ],
        "console": "internalConsole",
        "internalConsoleOptions": "openOnSessionStart",
        "outputCapture": "std",
        "env": {
          "FIORI_TOOLS_URL_PARAMS": "sap-ui-xx-viewCache=false",
          "run.config": "{\"handlerId\":\"fiori_tools\",\"runnableId\":\"/home/user/projects/lesson3\"}"
        }
      }
    ]
  }